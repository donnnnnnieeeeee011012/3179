{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Covid Status",
  "projection": {"type":"equalEarth"},
  "layer":[
    
    {
      "data": {
        "url": "https://raw.githubusercontent.com/donnnnnnieeeeee011012/3179/main/Homework%202/js/ne_110m_admin_0_countries.json",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark":{"type": "geoshape","stroke":"aliceblue"}
    },
    {
      "transform": [
        {
          "lookup": "properties.NAME",
          "from":{
            "data": {
              "url": "https://raw.githubusercontent.com/donnnnnnieeeeee011012/3179/main/Homework%202/data/2019.csv"
            },
            "key": "Country or region"
            
          }
        }
      ],
      "mark": {"type": "geoshape"},
      "encoding": {
        "color":{
          "field": "Country",
          "type": "nominal"
          
          }
        }
      } 
    }
  ]
}